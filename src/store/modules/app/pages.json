{
  "clientInfo": {
    "shortForm": "CI",
    "icon": "person",
    "validation": ["$.locations[0].number<10"],
    "bind": "",
    "layout": [
        [
          {
            "type": "TabbedSection",
            "bind": "locations",
            "layout": [
              [
                {
                  "label": "Size",
                  "type": "Number",
                  "bind": "size",
                  "width": 12
                },
                {
                  "label": "#",
                  "type": "Number",
                  "bind": "number",
                  "width": 12
                },
                {
                  "label": "City",
                  "type": "TextField",
                  "validation": "it.length>3",
                  "bind": "city",
                  "width": 12
                }
              ],
              [
                {
                  "label": "Country",
                  "type": "TextField",
                  "width": 12,
                  "bind": "country"
                }
              ]
            ]
          },
          {
            "label": "Are you under 18?",
            "type": "Checkbox",
            "bind": "under18",
            "width":12
          },
          {
            "label": "Name",
            "type": "TextField",
            "bind": "name",

            "width":12
          }
        ]
      ]
  },
  "fleet": {
    "shortForm": "F",
    "icon": "person",
    "label": "Fleet",
    "bind": "fleet",
    "ui": {
      "rows": [
        {
          "label": "Vehicle",
          "type": "TextField",
          "bind": "vehicle"
        }
      ]
    }
  }
}

